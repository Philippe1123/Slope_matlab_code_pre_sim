Merge "versionfive_steeper.geo";
Mesh.MshFileVersion = 2.2;
Mesh 2;


SetOrder 1;
Mesh.SaveTopology; 
Save Sprintf("level_%1g.msh",0);
Mesh.SaveTopology; 
Delete All;


Merge "versionfive_steeper.geo";
Mesh 2;

SetOrder 3;
Mesh.SaveTopology; 
Mesh.MshFileVersion = 2.2;
Save Sprintf("level_%1g.msh",1);
Delete All;

Merge "versionfive_steeper.geo";
Mesh 2;

SetOrder 4;
Mesh.SaveTopology; 
Mesh.MshFileVersion = 2.2;
Save Sprintf("level_%1g.msh",2);
Delete All;

Merge "versionfive_steeper.geo";
Mesh 2;

SetOrder 5;
Mesh.SaveTopology; 
Mesh.MshFileVersion = 2.2;
Save Sprintf("level_%1g.msh",3);
Delete All;



Exit;

